__author__ = "Umut Can Kocaman"

def tersCevir():
    liste = []
    sayac = -1
    x = input("Listenizi elemanlar arasinda bosluk olmayacak sekilde, bitisik giriniz\n")
    elemanSayisi = len(x)

    for i in range(elemanSayisi):
        liste.append(x[i])

    for i in range(elemanSayisi):
        y = liste.pop(-1) #listenin son elemanini silip ye degiskenine atadık
        liste.insert(sayac+1 , y) #y degerini listenin en başına ekledik
        elemanSayisi -= 1
        sayac += 1

    print("Listenin Tersi : {}".format(liste))
tersCevir()
